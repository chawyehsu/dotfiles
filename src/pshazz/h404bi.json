{
	"plugins": [ "git", "ssh", "z", "aliases" ],
	"prompt": [
		[ "darkgreen",   "", "$hostname" ],
		[ "darkcyan",    "", " $dir" ],
		[ "darkyellow",  "", " $git_lbracket$git_branch" ],
		[ "darkyellow",  "", " $git_dirty" ],
		[ "darkyellow",  "", "$git_rbracket"],
		[ "",            "", "`n$" ]
	],
	"git": {
		"prompt_lbracket": "(",
		"prompt_rbracket": ")"
	}
}