{
	"plugins": [ "git", "ssh", "z", "aliases", "virtualenv" ],
	"prompt": [
		[ "darkgreen",  "", "$hostname" ],
		[ "darkcyan",   "", " $dir" ],
		[ "darkyellow", "", " $git_lbracket$git_branch" ],
		[ "darkyellow", "", " $git_dirty" ],
		[ "darkyellow", "", "$git_rbracket"],
		[ "yellow",     "", " $virtualenv_lbracket$virtualenv"],
		[ "yellow",     "", "$virtualenv_rbracket"],
		[ "",           "", "`n$" ]
	],
	"git": {
		"prompt_lbracket": "(",
		"prompt_rbracket": ")"
	},
	"virtualenv": {
		"prompt_lbracket": "[",
		"prompt_rbracket": "]"
	}
}
