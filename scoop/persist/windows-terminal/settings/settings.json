{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [
        {
            "command":
            {
                "action": "copy",
                "singleLine": false
            },
            "id": "User.ActionCopy"
        },
        {
            "command":
            {
                "action": "scrollUp",
                "rowsToScroll": 5
            },
            "id": "User.ScrollUp"
        },
        {
            "command":
            {
                "action": "scrollDown",
                "rowsToScroll": 5
            },
            "id": "User.ScrollDown"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
    "disabledProfileSources":
    [
        "Microsoft.WSL",
        "Windows.Terminal.Wsl",
        "Windows.Terminal.Azure"
    ],
    "initialCols": 84,
    "initialRows": 28,
    "keybindings":
    [
        {
            "id": "Terminal.MoveFocusDown",
            "keys": "alt+shift+down"
        },
        {
            "id": "User.ActionCopy",
            "keys": "ctrl+c"
        },
        {
            "id": "Terminal.OpenNewTab",
            "keys": "ctrl+t"
        },
        {
            "id": "Terminal.MoveFocusUp",
            "keys": "alt+shift+up"
        },
        {
            "id": "Terminal.SplitPaneDown",
            "keys": "win+shift+d"
        },
        {
            "id": "User.ScrollUp",
            "keys": "alt+up"
        },
        {
            "id": "User.ScrollDown",
            "keys": "alt+down"
        },
        {
            "id": "Terminal.SplitPaneRight",
            "keys": "alt+shift+d"
        },
        {
            "id": "Terminal.MoveFocusLeft",
            "keys": "alt+shift+left"
        },
        {
            "id": "Terminal.MoveFocusRight",
            "keys": "alt+shift+right"
        },
        {
            "id": "Terminal.ResizePaneDown",
            "keys": "ctrl+shift+down"
        },
        {
            "id": "Terminal.ResizePaneLeft",
            "keys": "ctrl+shift+left"
        },
        {
            "id": "Terminal.ResizePaneRight",
            "keys": "ctrl+shift+right"
        },
        {
            "id": "Terminal.ResizePaneUp",
            "keys": "ctrl+shift+up"
        },
        {
            "id": "Terminal.ClosePane",
            "keys": "ctrl+w"
        },
        {
            "id": "Terminal.NextTab",
            "keys": "ctrl+right"
        },
        {
            "id": "Terminal.PrevTab",
            "keys": "ctrl+left"
        }
    ],
    "launchMode": "maximized",
    "newTabMenu":
    [
        {
            "type": "remainingProfiles"
        }
    ],
    "profiles":
